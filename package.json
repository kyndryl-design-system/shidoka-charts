{
  "name": "@kyndryl-design-system/shidoka-charts",
  "version": "0.0.0-semantic-release",
  "description": "Shidoka Charts",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kyndryl-design-system/shidoka-charts.git"
  },
  "scripts": {
    "dev": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "npm run powerbi:build && storybook build",
    "build": "rollup -c",
    "powerbi:build": "node build-powerbi-themes.cjs",
    "powerbi:validate": "node src/powerbi/scripts/validate-powerbi-themes.ci.cjs dist/powerbi/flat",
    "powerbi:manifest": "node src/powerbi/scripts/write-theme-manifest.cjs dist/powerbi/flat dist/powerbi/manifest.json",
    "powerbi:clean": "rm -rf dist/powerbi && mkdir -p dist/powerbi",
    "powerbi:all": "npm run powerbi:clean && npm run powerbi:build && npm run powerbi:validate && npm run powerbi:manifest",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "test": "vitest run --project=storybook",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "@kyndryl-design-system/shidoka-foundation": "^2.7.0",
    "@kyndryl-design-system/shidoka-icons": "^2.0.0",
    "@sgratzl/chartjs-chart-boxplot": "^4.4.4",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-chart-geo": "^4.2.4",
    "chartjs-chart-matrix": "^2.0.1",
    "chartjs-chart-treemap": "^2.3.0",
    "chartjs-plugin-a11y-legend": "^0.1.0",
    "chartjs-plugin-annotation": "^3.0.1",
    "chartjs-plugin-chart2music": "^0.1.2",
    "chartjs-plugin-datalabels": "^2.2.0",
    "date-fns": "^2.30.0",
    "deepmerge-ts": "^5.1.0",
    "lit": "^3.3.0",
    "powerbi-client": "^2.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@custom-elements-manifest/analyzer": "^0.9.4",
    "@rollup/plugin-dynamic-import-vars": "^2.0.5",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-a11y": "^9.0.6",
    "@storybook/addon-designs": "^10.0.1",
    "@storybook/addon-docs": "^9.0.6",
    "@storybook/addon-links": "^9.0.6",
    "@storybook/addon-themes": "^9.0.6",
    "@storybook/addon-vitest": "^9.0.6",
    "@storybook/web-components-vite": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/browser": "^3.2.3",
    "@vitest/coverage-v8": "^3.2.3",
    "archiver": "^7.0.1",
    "axe-playwright": "^2.0.1",
    "chromatic": "^11.2.0",
    "eslint": "^8.31.0",
    "eslint-plugin-storybook": "^9.0.6",
    "html2canvas": "^1.4.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "lit-analyzer": "^2.0.3",
    "playwright": "^1.55.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^4.0.0",
    "resolve-url-loader": "^5.0.0",
    "rollup": "^4.42.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-inline-svg": "^3.0.3",
    "rollup-plugin-multi-input": "^1.3.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-postcss-lit": "^2.2.0",
    "rollup-plugin-rename-node-modules": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.89.1",
    "sass-embedded": "^1.89.1",
    "storybook": "^9.0.6",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "customElements": "custom-elements.json",
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,json,css,scss,md,mdx}": "prettier --write"
  },
  "overrides": {
    "storybook": "$storybook"
  }
}
